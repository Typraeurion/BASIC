DEF F(X)=2*X^2+3*X+5
DEF MAX(A,B)=(A+B+ABS(A-B))/2
DEF MIN(A,B)=(A+B-ABS(A-B))/2

REM Volume of an ellipsoid
DEF VELLIPSE(R1,R2,R3)=4.18879020478639098458*R1*R2*R3

DEF RND$(X)=CHR$(INT(RND(26))+65)+CHR$(INT(RND(26))+65)+CHR$(INT(RND(26))+65)+CHR$(INT(RND(26))+65)+CHR$(INT(RND(26))+65)

PRINT "POLYNOMIAL F(1.234) =";F(1.234)
PRINT "MAX (1354, 1634) =";MAX(1354,1634)
PRINT "MIN (7869, 6785) =";MIN(7869, 6785)
PRINT "VOLUME OF ELLIPSE WITH RADII 5, 6, 7 =";VELLIPSE(5,6,7)

PRINT "YOUR RANDOM WORD: ";RND$(0)
